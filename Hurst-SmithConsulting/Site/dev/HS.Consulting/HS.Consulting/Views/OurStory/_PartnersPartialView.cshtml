<div class="row">
    <div class="col-xs-12">
        <div class="row">
            <div class="col-xs-12">
                We work with a range of trusted partners whose offering compliment ours, who we trust and most importantly who share our values.
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12"><br /></div>
        </div>
        <div class="row">
            <div class="col-xs-12"><hr /></div>
        </div>
        @foreach (var partner in HS.Consulting.Data.PartnerData.Partners)
        {
            <div class="row">
                <div class="col-xs-12"><br /></div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    @partner.Company
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12"><br /></div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <a href="@partner.Url">@partner.Url</a>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12"><br /></div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    @Html.Raw(@partner.Caption)
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12"><br /></div>
            </div>

            <div class="row">
                <div class="col-xs-12"><hr /></div>
            </div>
            <div class="row">
                <div class="col-xs-12"><br /></div>
            </div>
        }
    </div>
</div>